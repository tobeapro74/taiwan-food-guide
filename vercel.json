{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/styles.css",
            "dest": "/public/styles.css"
        },
        {
            "src": "/app.js",
            "dest": "/public/app.js"
        },
        {
            "src": "/sw.js",
            "dest": "/public/sw.js"
        },
        {
            "src": "/manifest.json",
            "dest": "/public/manifest.json"
        },
        {
            "src": "/favicon.svg",
            "dest": "/public/favicon.svg"
        },
        {
            "src": "/icons/(.*)",
            "dest": "/public/icons/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}
